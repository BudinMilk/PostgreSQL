<Window x:Class="PostgreSQL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PostgreSQL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Insert" HorizontalAlignment="Left" Margin="100,100,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Update" HorizontalAlignment="Left" Margin="200,100,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Delete" HorizontalAlignment="Left" Margin="300,100,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <Button Content="Select" HorizontalAlignment="Left" Margin="400,100,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.441,-0.351" Click="Button_Click_3"/>

    </Grid>
</Window>
